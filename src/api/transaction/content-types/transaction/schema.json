{
  "kind": "collectionType",
  "collectionName": "transactions",
  "info": {
    "singularName": "transaction",
    "pluralName": "transactions",
    "displayName": "Transaction",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "refId": {
      "type": "string",
      "unique": true
    },
    "amount": {
      "type": "float"
    },
    "project": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project.project"
    },
    "paidBy": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "transactionDate": {
      "type": "datetime"
    },
    "paymentStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "success",
        "failed",
        "canceled"
      ]
    },
    "method": {
      "type": "enumeration",
      "enum": [
        "stripe",
        "razorpay",
        "upi",
        "cash"
      ]
    }
  }
}
