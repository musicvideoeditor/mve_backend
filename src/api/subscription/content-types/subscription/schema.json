{
  "kind": "collectionType",
  "collectionName": "subscriptions",
  "info": {
    "singularName": "subscription",
    "pluralName": "subscriptions",
    "displayName": "Subscription",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "plan": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::plan.plan"
    },
    "expiresAt": {
      "type": "date"
    },
    "refId": {
      "type": "string"
    },
    "gateway": {
      "type": "enumeration",
      "enum": [
        "Stripe",
        "UPI"
      ]
    },
    "paidAt": {
      "type": "date"
    },
    "subscriptionStatus": {
      "type": "enumeration",
      "enum": [
        "pending",
        "subscribed",
        "cancelled",
        "expired"
      ],
      "default": "pending"
    }
